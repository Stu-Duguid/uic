{
	"name": "Tealeaf Event Builder",
	"version": "1.0",
	"manifest_version": 3,
	"description": "Helps build JSON to import events",
	"action": {
		"default_title": "Tealeaf Event Builder",
		"default_icon": {
			"32": "icons/inspection.jpg"
		},
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab", "scripting", "storage"],
	"background": {
		"service_worker": "background.js"
	}
}