    {
      "displayName": "Referring Data",
      "internalName": "E_REFERRING_DATA_666",
      "capturePersonalData": false,
      "active": true,
      "tags": [
        "referrer"
      ],
      "description": "Uses session referrer and query string to calculate detailed referrer channel, campaign, host, and normalised url",
      "trigger": "EveryStep",
      "publish": "Immediate",
      "mode": "Advanced",
      "javascript": "// regexes external to function to reduce compilation\nvar removeQuery = /\\?.*$/;\nvar extractHost = /^(?:https?:\\/\\/)?([^/]+).*$/i;\nvar searchHost = /google|bing|duckduck|yahoo/i;\n\n// NOTE: Do not change event name\nfunction E_REFERRING_DATA_666()\n{\n\tif ($P[\"TL.STEP_SCREENVIEW_TYPE\"].firstValue().toUpperCase() == \"LOAD\")\n\t{\n\t\tvar channel, campaign, host, url;\n\t\tvar dims = {};\n\t\tvar gclid = $P[\"P_STEP_GOOGLE_CLICK_IDENTIFIER_666\"].patternFound();\n\t\tvar dclid = $P[\"P_STEP_GOOGLE_DISPLAY_CLICK_IDENTIFIER_666\"].patternFound();\n\t\tvar campaignid = $P[\"P_STEP_GOOGLE_CAMPAIGN_IDENTIFIER_666\"].firstValue();\n\t\tvar utmSource = $P[\"P_STEP_UTM_SOURCE_666\"].firstValue().toUpperCase();\n\t\tvar utmMedium = $P[\"P_STEP_UTM_MEDIUM_666\"].firstValue().toUpperCase();\n\t\tvar utmCampaign = $P[\"P_STEP_UTM_CAMPAIGN_666\"].firstValue().toUpperCase();\n\t\t\n\t\tcampaign = utmCampaign;\n\n\t\tif (gclid && campaignid !== \"\") {\n\t\t\tchannel = \"Paid Search\";\n\t\t\tcampaign = \"Google\";\n\t\t}\n\t\telse if (utmSource == \"BING\" && utmMedium == \"CPC\") {\n\t\t\tchannel = \"Paid Search\";\n\t\t\tcampaign = \"Bing: \"+utmCampaign;\n\t\t}\n\t\telse if (gclid && dclid) {\n\t\t\tchannel = \"Display\";\n\t\t\tcampaign = \"Google\";\n\t\t}\n\t\telse if (utmSource == \"FACEBOOK\" && utmMedium == \"SOCIAL\") {\n\t\t\tchannel = \"Social\";\n\t\t\tcampaign = \"Facebook: \"+utmCampaign;\n\t\t}\n\t\telse if (utmSource == \"FACEBOOK\" && utmMedium == \"SOCIAL_DISPLAY\") {\n\t\t\tchannel = \"Display\";\n\t\t\tcampaign = \"Facebook: \"+utmCampaign;\n\t\t}\n\t\telse if (utmMedium.indexOf(\"EMAIL\") !== -1) {\n\t\t\tchannel = \"Email\";\n\t\t}\n\t\telse {\n\t\t\tcampaign = \"\";\n\t\t\turl = $F.getLastEventValue(\"E_SESSION_REFERRER_666\").replace(removeQuery, \"\");\n\t\t\thost = url.replace(extractHost, \"$1\");\n\t\t\tif (url == \"\") {\n\t\t\t\tchannel = \"Direct\";\n\t\t\t}\n\t\t\telse if (host.match(searchHost)) {\n\t\t\t\tchannel = \"Organic Search\";\n\t\t\t}\n\t\t\telse {\n\t\t\t\tchannel = \"Referral\";\n\t\t\t}\n\t\t}\n\n\t\tdims[\"DIM_REFERRING_CHANNEL_666\"] = channel;\n\t\tdims[\"DIM_REFERRING_CAMPAIGN_666\"] = campaign;\n\t\tdims[\"DIM_REFERRING_HOST_666\"] = host;\n\t\tdims[\"DIM_REFERRING_URL_666\"] = url;\n\n\t\t$F.setFacts(\"E_REFERRING_DATA_666\", null, dims);\n\t}\n}\n",      "referenceValueType": "Custom",
      "valueType": "Text",
